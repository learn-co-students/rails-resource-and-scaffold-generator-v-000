<p id="notice"><%= notice %></p>

<h1>Listing Artes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artes.each do |arte| %>
      <tr>
        <td><%= arte.title %></td>
        <td><%= arte.body %></td>
        <td><%= link_to 'Show', arte %></td>
        <td><%= link_to 'Edit', edit_arte_path(arte) %></td>
        <td><%= link_to 'Destroy', arte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Arte', new_arte_path %>
